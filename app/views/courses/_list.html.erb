<div id="filterrific_results">
  <table class='table table-striped table-hover table-responsive'>
    <thead>
      <tr>
        <th>Apelido</th>
        <th>Nome</th>
        <th>Semestre</th>
      </tr>
    </thead>
    <tbody>
      <% courses.each do |course| %>
        <tr href='/provas/<%= course.alias %>'>
          <td><%= course.alias %></td>
          <td><%= course.name %></td>
          <td><%= course.semester %></td>
        </tr>
      <% end %>
    <tbody>
  </table>
  <%= paginate @courses %>
</div>